query{
  computeResultant(accelerations:[
    {
      id: "accel1",
      x: 1.0
      y: 1.0
      z: 1.0
    }
    {
      id: "accel2",
      x: 2.0
      y: 2.0
      z: 2.0
    }
  ]){
    id
    value
  }
}


query {
  makeButterworthFilter(filter:{
    id: "filter",
    filterOrder: 5,
    filterFrequencies:{
      id: "freqs",
      highCutOff: {
        id: "hc",
        value: 6.0
        unit: "hz"
      }
      lowCutOff:  {
        id: "lc",
        value: 0.1
        unit: "hz"
      }
      samplingFrequency: {
        id: "fs",
        value: 20
        unit: "hz"
      }
    }
    function:{
      id: "band"
    }
  }){
    id
    numerator
    denominator
  }
}

query {
  lfilter1D(iirFilterPolynomials: {
    id: "filter",
    numerator: [1.0, 1.0],
    denominator: [2.0,2.0]
  }
  dataToFilter:[{
    id: "id"
    value: 1.0
  },
    {
    id: "id"
    value: 1.0
  }
  ]){
    id
    values
  }
}

query {
  compute1DDFT(
    input: {
      id:"mag",
      values: [1.0, 1.0, 1.0]
    }
    points: 512
  ){
    id
    value
  }
}

query {
  computeIntensity(
    fftMagnitudes: [{
      id: "mag",
      value: 0.0117
    },
      {
      id: "mag1",
      value: 0.0117
    }
    ]
    fftpoints:512
    filter: {
      id: "filter"
      filterOrder: 5
      function: {
        id:"band"
      }
      filterFrequencies:{
        id: "freqs"
        lowCutOff:{
          id: "low"
          value: 0.1
          unit: "hz"
        }
        highCutOff:{
          id: "high"
          value: 6
          unit: "hz"
        }
        samplingFrequency:{
          id: "fs"
          value:20
          unit: "hz"
        }
      }
    }
  ){
    id
    value
  }
}